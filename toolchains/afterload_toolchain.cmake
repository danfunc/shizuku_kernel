if(DEFINED ENV{SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN}
   AND NOT SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN)
  set(SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN $ENV{SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN})
endif()
if(EXISTS
   ${CMAKE_CURRENT_SOURCE_DIR}/toolchains/${SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN})
  include(
    ${CMAKE_CURRENT_SOURCE_DIR}/toolchains/${SHIZUKU_KERNEL_AFTERLOAD_TOOLCHAIN})
else()
  message(
    "[warning] SHIZUKU_AFTERLOAD_TOOLCHAIN NOT FOUND. It maybe cause cmake configuration error."
  )
endif()

