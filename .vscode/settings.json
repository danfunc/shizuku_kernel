{
    "git.repositoryScanIgnoredFolders": [
        "node_modules",
        "**/build"
    ],
    "git.autofetch": true,
    "git.autoRepositoryDetection": true,
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "build"
    ],
    "micropico.pyIgnore": [
        "**/.picowgo",
        "**/.micropico",
        "**/.vscode",
        "**/.gitignore",
        "**/.git",
        "**/.DS_Store",
        "**/project.pico-go",
        "**/env",
        "**/venv",
        "**/.venv",
        "**/build"
    ],
    "githubIssues.ignoreCompletionTrigger": [
        "coffeescript",
        "diff",
        "dockerfile",
        "dockercompose",
        "ignore",
        "ini",
        "julia",
        "makefile",
        "perl",
        "powershell",
        "python",
        "r",
        "ruby",
        "shellscript",
        "yaml",
        "build"
    ],
    "explorer.excludeGitIgnore": false
}